<h1>Welcome!</h1>
<p>Please Enter your Latitude and Longitude</p>
<%= form_tag solar_data_solar_data_path do %>
  <div>
    <%= label_tag :latitude %>
    <%= text_field_tag :latitude %>
  </div>
  <div>
    <%= label_tag :longitude %>
    <%= text_field_tag :longitude %>
  </div>

  <%= submit_tag %>
<% end %>
